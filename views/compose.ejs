<%- include('partials/header.ejs') -%>
<h1>Compose</h1>
<form action="/compose" method="post" class="form">
    <div class="form-group">
        <label for="posttitle">Post Title</label>
        <input class="form-control" type="text" name="postTitle" id="posttitle" placeholder="Post Title">
    </div>
    <div class="form-group">
        <label for="post">Post</label>
        <textarea class="form-control" name="post" id="post" rows="4"></textarea>
    </div>
    <div class="form-group">
        <button class="btn btn-primary" type="submit" name="button">Publish</button>
    </div>
</form>